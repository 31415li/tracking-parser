{
  "name": "tracking-parser",
  "version": "0.4.3",
  "description": "Parse raw data from tracking devices",
  "main": "src",
  "scripts": {
    "lint": "eslint src",
    "test": "mocha",
    "coveralls": "coveralls < coverage/lcov.info"
  },
  "engines": {
    "node": ">=0.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lgaticaq/tracking-parser.git"
  },
  "keywords": [
    "tz-avl",
    "meitrack",
    "avl",
    "gps",
    "tracking",
    "parser"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/tracking-parser/issues"
  },
  "homepage": "https://github.com/lgaticaq/tracking-parser#readme",
  "dependencies": {
    "bluebird": "^3.3.4",
    "bscoords": "0.0.1",
    "meitrack-parser": "0.3.3",
    "simple-reverse-geocoder": "^1.1.0",
    "tz-parser": "^2.4.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "2.12.0",
    "mocha": "^2.4.5"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        2,
        2
      ],
      "linebreak-style": [
        2,
        "unix"
